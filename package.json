{
  "name": "H2Q",
  "version": "1.0.0",
  "description": "HHA to Quickbooks",
  "main": "server.js",
  "scripts": {
    "start": "dotenv -- nodemon -e ts,graphql -x ts-node src/server.ts",
    "debug": "dotenv -- nodemon -e ts,graphql -x ts-node --inspect src/server.ts",
    "build": "rimraf dist && tsc"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start",
    "ext": "ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/puppeteer": "^1.19.1",
    "@types/ramda": "^0.26.21",
    "chalk": "^2.4.2",
    "dotenv": "^8.0.0",
    "moment": "^2.29.2",
    "moment-timezone": "^0.5.26",
    "node-uuid": "^1.4.8",
    "puppeteer": "^1.19.0",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/node": "^12.7.1",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.18.2",
    "nodemon": "^1.19.1",
    "rimraf": "^2.6.3",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  }
}
